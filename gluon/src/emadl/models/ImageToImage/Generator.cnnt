configuration Network{
     num_epoch:1
     batch_size:32
     normalize:false
     context:gpu
     load_checkpoint:true
     loss:dice_loss{
        loss_axis:1
     }
     optimizer:sgd{
         learning_rate:0.00001
         learning_rate_decay:0.95
         step_size:450
         weight_decay:0.0
     }
}
