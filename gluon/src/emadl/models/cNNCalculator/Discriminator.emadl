package cNNCalculator;

component Discriminator{
    ports in Q(-1:1)^{1, 28, 28} image,
         out Q(-oo:oo)^{1} dis;

    implementation CNN {

        image ->
        Convolution(kernel=(4,4),channels=64, stride=(2,2)) ->
        Convolution(kernel=(4,4),channels=128, stride=(2,2)) ->
		BatchNorm() ->
		FullyConnected(units=1024, no_bias=true) ->
		BatchNorm() ->
		Relu() ->
		FullyConnected(units=1, no_bias=true) -> 
		dis;
    }
}
