configuration Generator{

	 learning_method:gan
	 discriminator_name: cNNCalculator.Discriminator
     num_epoch:20
     batch_size:10
     normalize:false
     context:cpu
     load_checkpoint:false
	 loss:sigmoid_binary_cross_entropy
     optimizer:adam{
         learning_rate:0.0002
         beta1:0.5
     }
	 noise_distribution:gaussian{
		 mean_value:0
		 spread_value:1
	 }
}
