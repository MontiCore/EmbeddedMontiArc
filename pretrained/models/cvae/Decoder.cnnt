configuration Decoder{
     learning_method: generative_model
     gm_algorithm: vae
     encoder_name: mnistvae.Encoder
     num_epoch:50
     batch_size:400
     normalize:false
     context:cpu
     load_checkpoint:false
	 loss: gaussian_vae_loss
     network_loss_params : ["mean", "loss"]
     use_labels: True
     reparametrize: cvae.reparametrize.

     optimizer:adam{
          learning_rate:0.003
	     weight_decay:0.01
     }
}
