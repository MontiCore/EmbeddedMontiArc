configuration Decoder{
     learning_method: generative_model
     gm_algorithm: vae

     encoder_name: mnistvae.Encoder
     num_epoch:1
     batch_size:200
     context:cpu
     print_images: true
     kl_loss_weight:1
     labeled_training: false
     label_classes: 10

     optimizer: sgd{
          learning_rate:0.003
	     weight_decay:0.01
     }
}
