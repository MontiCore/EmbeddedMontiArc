package tictactoe.agent.network;

component TicTacToeQNet {
    ports
        in Q(0:1)^{29} state,
        out Q(-oo:oo)^{9} qvalues; 

    implementation CNN {
        state ->
        Relu() ->
        FullyConnected(units=29) ->
        Relu() ->
        FullyConnected(units=27) ->
        Relu() ->
        FullyConnected(units=27) ->
        Relu() ->
        FullyConnected(units=9) ->
        qvalues;
    }
}