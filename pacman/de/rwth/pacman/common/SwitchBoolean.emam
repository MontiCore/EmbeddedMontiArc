package de.rwth.pacman.common;

component SwitchBoolean { // if no generic is given, then default type B is taken
	port 
		in Boolean in1, //if
	    in Boolean cond, // condition
		in Boolean in3, //else
		out Q out1;

	implementation Math{
	    if cond
	        out1=in1;
	    else
	        out1=in3;
	    end
	}
}
