configuration SmallSeg{
     num_epoch:1
     batch_size:32
     normalize:false
     context:gpu
     load_checkpoint:false
     loss:softmax_cross_entropy{
        loss_axis:1
     }
     optimizer:sgd{
         learning_rate:0.1
         learning_rate_decay:0.85
         step_size:1000
         weight_decay:0.0
     }
}
