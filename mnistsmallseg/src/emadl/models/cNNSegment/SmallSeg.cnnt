configuration SmallSeg{
     num_epoch:10
     batch_size:32
     normalize:false
     context:gpu
     load_checkpoint:true
     loss:softmax_cross_entropy{
        loss_axis:1
     }
     optimizer:sgd{
         learning_rate:0.5
         learning_rate_decay:0.85
         step_size:750
         weight_decay:0.0
     }
}
