component Alexnet{
    ports in Z(0:oo)^{224, 224, 3} image,
         out Q(0:1) predictions[10];

    implementation DeepLearning {

        architecture: {image} ->
                      Alexnet(img_height=224, img_width=224, img_channels=3, classes=10)
                      -> {predictions}

        configuration: FullConfig(trainData="./data/mnist_train_data",
                                  trainLabel="./data/mnist_train_label",
                                  testData="./data/mnist_test_data",
                                  testLabel="./data/mnist_test_label")

    }
}