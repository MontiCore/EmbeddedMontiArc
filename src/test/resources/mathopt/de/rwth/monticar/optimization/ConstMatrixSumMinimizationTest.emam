package de.rwth.monticar.optimization;

component ConstMatrixSumMinimizationTest{

    ports out Q xOut,
          out Q yOut;

    implementation Math{
        Q y = minimize(Q x)
            sum([0, 0, x, 0]);
        subject to
            -1 <= x <= 1;
        end
        xOut = x;
        yOut = y;
    }
}