package de.rwth.monticar.optimization;

component ColRowMinTest{

    ports out Q^{2,2} xOut,
          out Q yOut;

    implementation Math{
        Q y = minimize(Q^{2,2} x)
          x(1, :) * x(:, 1);
        subject to
            x(1, 1) >= 1;
            x(1, 2) >= 2;
            x(2, 1) >= 1;
            x(2, 2) >= 1;
        end
        xOut = x;
        yOut = y;
    }
}