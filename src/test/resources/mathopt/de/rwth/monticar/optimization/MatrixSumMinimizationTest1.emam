package de.rwth.monticar.optimization;

component MatrixSumMinimizationTest1{

    ports out Q^{2,2} xOut,
          out Q yOut;

    implementation Math{
        minimize
            Q^{2,2} x;
        in
            Q y = sum(x);
        subject to
            x(1,1) >= 0;
            x(1,2) >= 0;
            x(2,1) >= 0;
            x(2,2) >= -5;
        end
        xOut = x;
        yOut = y;
    }
}
