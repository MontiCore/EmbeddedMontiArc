configuration FullConfig(trainData, trainLabel, testData, testLabel){
     train_data:trainData
     train_label:trainLabel
     test_data:testData
     test_label:testLabel
     num_epoch : 5
     batch_size : 100
     loading_mode:load_and_train
     eval_metric:ce
     checkpoint:1000
     optimizer:rmsprop{
         learning_rate:0.001
         weight_decay:0.01
         learning_rate_decay:0.9
         learning_rate_policy:step
         step_size:1000
         rescale_grad:1.1
         clip_gradient:10
         gamma1:0.9
         gamma2:0.9
         epsilon:0.000001
         centered:true
         clip_weights:10
     }
 }