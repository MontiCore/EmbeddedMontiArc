package mnist;
import Network;
import CalculateClass;

component Main{
    ports in Z(0:255)^{1, 28, 28} image,
         out Z(0:9) digit;

    instance Network(10) net;

    instance CalculateClass outCalc;

    connect image -> net.data;
    connect net.predictions -> outCalc.probabilities;
    connect outCalc.digit -> digit;

    }
}