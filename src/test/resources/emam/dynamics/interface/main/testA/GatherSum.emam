package testA;

dynamic component GatherSum {
	port
		dynamic in Q in1[0:32],
		out Q out1;

    @ in1::connect {
    // empty !
    }

    implementation Math{
        out1 = 0;
        for i=1:32
            if is_connected(in1,i)
                out1 = out1 + in1(i);
            end
        end
    }

}