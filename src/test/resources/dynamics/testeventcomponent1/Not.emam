package testeventcomponent1;



component Not {
    ports
        in B a,
        out B b,
        out B c;

    connect false -> c;

    @ a::value(false){
        connect true -> b;
    }

    @ a::value(true){
        connect false -> b;
    }

    @ a::value( [true, true, false ,false] ){
        connect true -> c;
    }

}