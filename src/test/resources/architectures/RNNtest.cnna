architecture RNNtest(max_length=50, vocabulary_size=30001, hidden_size=500) {
    def input Q(-oo:oo)^{10} source[2]
    def output Q(-oo:oo)^{10} target[2]

    layer RNN(units=10) encoder;
    layer RNN(units=10) decoder;

    source[0] ->
    encoder ->
    target[0];

    encoder.state ->
    decoder.state;

    source[1] ->
    decoder ->
    target[1];
}
