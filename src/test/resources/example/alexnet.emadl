component AlexNet<Z(1:oo) heigth, Z(1:oo) width, Z(1:oo) channels, Z(1:oo) outDims> {
    ports in Z(1:oo)^{heigth, width, channels} image,
         out Q predictions[outDims];

    implementation DeepLearning {

        context = gpu
        architecture = image --> alexnet --> predictions
        training = test

    }
}