architecture RNNencdec{
    def input Z(0:49999)^{30} source
    def output Z(0:49999)^{1} target[30]

    layer LSTM(units=1000) encoder;

    source ->
    Embedding(output_dim=620) ->
    encoder;

    1 -> target[0];

    layer LSTM(units=1000) decoder;

    encoder.state -> decoder.state;

    timed<t> GreedySearch(max_length=30) {
        target[t-1] ->
        Embedding(output_dim=620) ->
        decoder ->
        FullyConnected(units=50000) ->
        Softmax() ->
        ArgMax() ->
        target[t]
    };
}