configuration FullConfig2{
     num_epoch : 10
     batch_size : 100
     load_checkpoint : false
     context : gpu
     eval_metric : top_k_accuracy
     loss: huber{
        rho: 0.9
     }
     normalize : false
     optimizer : adam{
         learning_rate : 0.001
         learning_rate_minimum : 0.001
         weight_decay : 0.01
         learning_rate_decay : 0.9
         learning_rate_policy : exp
         step_size : 1000
         rescale_grad : 1.1
         clip_gradient : 10
         beta1 : 0.9
         beta2 : 0.9
         epsilon : 0.000001
     }
}
