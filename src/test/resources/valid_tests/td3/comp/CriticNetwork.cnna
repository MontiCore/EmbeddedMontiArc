architecture CriticNetwork {
    def input Q(-oo:oo)^{8} state
    def input Q(-1:1)^{3} action
    def output Q(-oo:oo)^{1} qvalues

    (state ->
        FullyConnected(units=300) ->
        Relu() ->
        FullyConnected(units=600)
    |

    action ->
        FullyConnected(units=600)
    ) ->
    Add() ->
    FullyConnected(units=600) ->
    Relu() ->
    FullyConnected(units=1) ->
    qvalues;
}