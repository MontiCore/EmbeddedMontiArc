configuration VAETestNet{
     num_epoch : 5
     batch_size : 100
     load_checkpoint : true
     eval_metric : mse
     loss: softmax_cross_entropy{
        loss_axis: -1
        sparse_label: true
        from_logits: false
     }
     context : cpu
     normalize : true
     optimizer : adam{
         learning_rate : 0.001
         learning_rate_minimum : 0.00001

     }
}
