architecture Encoder{
    def input Q(-oo:oo)^{1,28,28} data
    def output Q(-oo:oo)^{2} encoding

    data ->
    VectorQuantize(num_embeddings=64,beta=0.25) ->
    FullyConnected(units=400) ->
    FullyConnected(units=4) ->
    Split(n=2) ->
    (
        [0]
        |
        [1]
    ) ->
    Reparameterize(pdf="normal") ->
    encoding;
}