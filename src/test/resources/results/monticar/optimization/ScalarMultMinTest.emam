package de.rwth.monticar.optimization;

component ScalarMultMinTest{

    ports out Q xOut,
          out Q yOut;

    implementation Math{

        minimize
            Q x;
        in
            Q y = (([1,1] * [2;2]) * x);
        subject to
            x >= 5;
        end
        xOut = x;
        yOut = y;

        // test result
        // Q eps = 1E-5;
        // assert(x = 5);
        // assert(y = 20);
    }
}
