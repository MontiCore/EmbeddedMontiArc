package basicLibrary;


component Switch<T=Boolean> { // if no generic is given, then default type Boolean is taken
	port 
		in T in1, //if
	    in Boolean cond, // condition
		in T in3, //else
		out T out1;

	implementation Math{
	    if cond {
	        out1=in1;
	    }else{
	        out1=in3;
	    }
	    end
	}
}
