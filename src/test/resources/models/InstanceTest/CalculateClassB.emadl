package InstanceTest;

component CalculateClassB{
    ports in Q(0:1)^{10} probabilities,
          out Z(0:9) digit;

    implementation Math{
         Q(0:1:9) max = 0;
         Q maxValue = 0.0;

         for i = 1:10
            Q prob = probabilities(1, i);
            if prob > maxValue
                max = i - 1;
                maxValue = prob;
            end
         end

         digit = max;
    }

}
