package letterpred;


component Network<Z(0:oo) n=26> {
    ports in Z(0:255)^{1, 28, 28} data,
         out Q(0:1)^{n} softmax;

    instance Net1 net1;
    instance Net2<26> net2;


    connect data -> net1.data;
    connect net1.dataOutput -> net2.dataInput;
    connect net2.softmax -> softmax;
}
