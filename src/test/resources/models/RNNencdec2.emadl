component RNNencdec2{
   ports in Q(0:1)^{30000} source,
        out Q(0:1)^{30000} target[5];

   implementation CNN{

       source -> Softmax() -> target[0];

       timed <t=0> BeamSearchStart(max_length=4){
           target[t] ->
           FullyConnected(units=30000) ->
           Softmax() ->
           target[t+1]
       };


       }
 }
