package de.monticore.lang.monticar.semantics.loops;

component OscillationAsSpecification {
    port
        out Q x;

    implementation Math {
        symbolic Q v;

        x(t=0) = 2;
        v(t=0) = 0;

        diff(x) == v;
        diff(v) == -2/5 * x;
    }
}