package de.monticore.lang.monticar.semantics.loops;

component Oscillation {
    port
        out Q out1;

    instance de.monticore.lang.monticar.semantics.basicLibrary.Integration<Q>{out1 = 2} y;
    instance de.monticore.lang.monticar.semantics.basicLibrary.Integration<Q>{out1 = 0} yDiff;
    instance de.monticore.lang.monticar.semantics.basicLibrary.Gain<Q>(-2/5) yDiffDiff;

    connect y.out1 -> yDiffDiff.in1, out1;
    connect yDiffDiff.out1 -> yDiff.in1;
    connect yDiff.out1 -> y.in1;
}