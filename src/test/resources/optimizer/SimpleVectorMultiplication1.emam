package optimizer;

component SimpleVectorMultiplication1{
    ports in Q(-oo:oo)^{400,200} mat1,
          in Q(-oo:oo)^{200,100} mat2,
          in Q(-oo:oo)^{100,10} mat3,
          out Q(-oo:oo)^{10,200} matOut;


    implementation Math{
        matOut = mat1(:,1)*mat2(1,:)*mat3(:,1);
    }

}
