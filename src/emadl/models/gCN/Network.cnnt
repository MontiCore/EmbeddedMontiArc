configuration Network{
     num_epoch:20
     batch_size:1
     normalize:false
     context:cpu
     eval_metric:accuracy_ignore_label
     load_checkpoint:false
     train_mask:[0, 140]
     test_mask:[140, 1400]
     loss:softmax_cross_entropy_masked{
        sparse_label:true
     }
     optimizer:adam{
         learning_rate:0.1
         learning_rate_decay:0.95
         step_size:1
         weight_decay:0.0
     }
}
