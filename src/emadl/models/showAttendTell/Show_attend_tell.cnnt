configuration Show_attend_tell{
     num_epoch:2
     batch_size:2
     context:cpu
     eval_metric:bleu
     loss:softmax_cross_entropy
     save_attention_image:false
     optimizer:adam{
         learning_rate:0.005
         learning_rate_decay:0.9
         step_size:1000
         weight_decay:0.0001
     }
}
