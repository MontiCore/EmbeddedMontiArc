package cheetah.agent;

component CheetahCritic {
    ports
        in Q^{26} state,
        in Q(-1:1)^{6} action,
        out Q(-oo:oo)^{1} qvalue;
    
    implementation CNN {
        (state | action) -> 
        Concatenate() ->
        FullyConnected(units=400) ->
        Relu() ->
        FullyConnected(units=300) ->
        Relu() ->
        FullyConnected(units=1) ->
        qvalue;
    }
}