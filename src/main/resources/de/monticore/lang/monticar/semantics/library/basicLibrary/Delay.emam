package de.monticore.lang.monticar.semantics.library.basicLibrary;

non-df component Delay<T>(T dvalue = 0) {
	port 
		in T input,
		out T output;
	
	implementation Math {
	    static delayed = dvalue;
        output = dvalue;
        dvalue = input;
	}
}
