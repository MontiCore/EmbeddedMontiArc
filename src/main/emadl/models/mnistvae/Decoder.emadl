package mnistvae;

component Decoder{
    ports in Q(-oo:oo)^{8} data,
          out Q(-oo:oo)^{1,28,28} softmax;

    implementation CNN {
        data ->
        FullyConnected(units=50) ->
        FullyConnected(units=400) ->
        Relu() ->
        Dropout() ->
        FullyConnected(units=400) ->
        Relu() ->
        Dropout() ->
        FullyConnected(units=784) ->
        Reshape(shape=(1,28,28)) ->
        Sigmoid() ->
        softmax;
    }
}