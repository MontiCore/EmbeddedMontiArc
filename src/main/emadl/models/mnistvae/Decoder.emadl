package mnistvae;

component Decoder{
    ports in Q(-oo:oo)^{2} z,
          out Q(-oo:oo)^{1,28,28} z_decoded;

    implementation CNN {    
        z ->
        FullyConnected(units=3136) ->
        Reshape(shape=(64,7,7)) ->
        UpConvolution(kernel=(3,3), channels=64, stride=(2,2)) ->
        UpConvolution(kernel=(3,3), channels=32, stride=(2,2)) ->
        UpConvolution(kernel=(3,3), channels=1, stride=(2,2)) ->
        z_decoded;
    }
}