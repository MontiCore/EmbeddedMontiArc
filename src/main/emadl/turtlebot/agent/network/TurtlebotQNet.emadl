package turtlebot.agent.network;

component TurtlebotQNet {
    ports
        in Q(-oo:oo)^{10} state,
        out Q(-oo:oo)^{3} qvalues;


    implementation CNN {
        state ->
        FullyConnected(units=300) ->
        Relu() ->
        FullyConnected(units=600) ->
        Relu() ->
        FullyConnected(units=3) ->
        qvalues;
    }
}
