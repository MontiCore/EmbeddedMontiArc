package de.rwth.pacman.heithoff2.BFS.start;

component StartRight{
    ports
        in Q(0m: 20m) pacManX,
        in Q(0m: 23m) pacManY,
        out Q(0m: 20m) startX,
        out Q(0m: 23m) startY,
        out Z startD;
        
    implementation Math {
        startX = round(pacManX + 0.51); 
        startY = round(pacManY);
        startD = 3;
    }
}
