package cNNCalculator;

component Connector {
    ports in Q(0:1)^{62, 1, 1} noise,
		  in Z(0:9)^{10,1,1} c1,
          out Q(0:1)^{1, 28, 28} res;

    instance Generator predictor;
    connect noise -> predictor.data;
	connect c1 -> predictor.c1;
    connect predictor.softmax -> res;
}
