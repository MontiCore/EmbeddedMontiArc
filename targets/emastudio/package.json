  "scripts": {
    "logo": "node scripts/logo",
    "prepare": "yarn run clean && yarn run compile",
    "clean": "yarn run logo && theia clean && rimraf dist && rimraf app && lerna run --scope \"@emastudio/!(target-)*\" --parallel clean",
    "build": "yarn run logo && theia build --config configs/webpack.config.js",
    "build:development": "yarn build --mode development",
    "compile": "yarn run logo && lerna run --scope \"@emastudio/!(target-)*\" compile",
    "compile:clean": "yarn run logo && lerna run --scope \"@emastudio/!(target-)*\" --parallel compile:clean",
    "docs": "yarn run logo && lerna run --scope \"@emastudio/!(target-)*\" --parallel docs",
    "docs:clean": "yarn run logo && lerna run --scope \"@emastudio/!(target-)*\" --parallel docs:clean",
    "watch": "yarn build --mode development --watch",
    "start": "yarn run logo && theia start --startup-timeout -1",
    "package": "node scripts/package"
  },
  "devDependencies": {
    "@theia/cli": "latest",
