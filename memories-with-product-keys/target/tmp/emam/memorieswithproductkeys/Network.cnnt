configuration Network{
     num_epoch:1
     batch_size:5
     normalize:false
	 log_period:10
     context:cpu
	 shuffle_data:true
     load_checkpoint:false
	 loss:cross_entropy
     optimizer:adam{
         learning_rate:0.00003
		 weight_decay:0.01
     }
}
