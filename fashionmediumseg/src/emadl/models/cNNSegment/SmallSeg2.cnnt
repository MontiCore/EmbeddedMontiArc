configuration SmallSeg2{
     num_epoch:5
     batch_size:16
     normalize:false
     context:gpu
     load_checkpoint:true
     loss:dice_loss{
        loss_axis:1
     }
     optimizer:sgd{
         learning_rate:0.01
         learning_rate_decay:0.9
         step_size:750
         weight_decay:0.0
     }
}
