configuration Network{
     num_epoch:5
     batch_size:100
     normalize:true
     context:cpu
     load_checkpoint:false
     optimizer:sgd{
         learning_rate:0.2
         learning_rate_decay:0.9
         step_size:1000
         weight_decay:0.0
     }
}
