package de.rwth.pacman.heithoff2.BFS;

component StartBottom{
    ports
        in Z(-1m: 19m) pacManX,
        in Z(0m: 22m) pacManY,
        out Z(-1m: 19m) startX,
        out Z(0m: 22m) startY,
        out Z startD;
        
    implementation Math {
        startX = pacManX;
        startY = round(pacManY + 0.51);
        startD = 1;
    }
}