package torcs.agent.network;

component TorcsActor {
    ports
        in Q^{29} state,
        out Q(-1:1)^{3} commands;

    implementation CNN {
        state ->
        FullyConnected(units=300) ->
        Relu() ->
        FullyConnected(units=600) ->
        Relu() ->
        FullyConnected(units=3) ->
        Tanh() ->
        commands;
    }
}
