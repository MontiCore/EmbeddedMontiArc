package de.rwth.montisim.agent.network;

component AutopilotQNet {
	ports
		in Q^{50} state,
		out Q(-1:1)^{3} action;

	implementation CNN {
		state ->
		FullyConnected(units=512) ->
		Relu() ->
		Dropout()->
		FullyConnected(units=512) ->
		Relu() ->
		Dropout()->
		FullyConnected(units=256) ->
		Relu() ->
		Dropout()->
		FullyConnected(units=3) ->
		Tanh() ->
		action;
	}
}
