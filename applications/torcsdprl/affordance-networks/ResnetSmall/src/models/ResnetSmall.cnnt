configuration ResnetSmall{
     load_checkpoint : true
     num_epoch : 1
     batch_size : 64
     context : cpu
     normalize : false
     loss : l2
     shuffle_data : true
     eval_metric : rmse
     log_period : 1
     checkpoint_period : 1
     optimizer : adam{
          learning_rate : 0.001
          beta1 : 0.9
          beta2 : 0.999
          epsilon : 1.0E-6
     }
}