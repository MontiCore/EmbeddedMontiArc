package de.rwth.montisim.agent.network;

component AutopilotQNet {
    ports
        in Q^{25} state,
        out Q(-1:1)^{3} action;

    implementation CNN {
		state ->
		FullyConnected(units=256) ->
		Relu() ->
		FullyConnected(units=384) ->
		Relu() ->
		FullyConnected(units=3) ->
		Tanh() ->
		action;
    }
}
