package de.rwth.montisim.agent.network;

component AutopilotQNet {
	ports
		in Q^{25} state,
		out Q(-1:1)^{2} action;

	implementation CNN {
		state ->
		FullyConnected(units=512) ->
		Relu() ->
		FullyConnected(units=1024) ->
		Relu() ->
		FullyConnected(units=256) ->
		Relu() ->
		FullyConnected(units=2) ->
		Tanh() ->
		action;
	}
}
