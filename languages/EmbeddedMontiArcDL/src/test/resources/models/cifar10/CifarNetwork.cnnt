configuration CifarNetwork{
     num_epoch:10
     batch_size:64
     normalize:true
     context:gpu
     load_checkpoint:false
     optimizer:adam{
         learning_rate:0.01
         learning_rate_decay:0.8
         step_size:1000
         weight_decay:0.0001
     }
}
