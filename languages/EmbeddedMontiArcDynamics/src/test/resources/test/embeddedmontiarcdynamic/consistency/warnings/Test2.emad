package warnings;

dynamic component Test2 {
    port
        in B in0,
        in B in1,
        in Q in2,
        out B out0,
        out B out1;


@in2::value(>500) && in1::value(true) && in0::value(true){
    connect true -> out0;
}

@in0::value(true) && in1::value(true) && in2::value(>500){
    connect true -> out1;
}
}