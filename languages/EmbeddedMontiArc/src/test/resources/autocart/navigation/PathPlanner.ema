package autocart.navigation;

component PathPlaner((0:1:oo) n){
    port
        in (-1:1:oo) destination,
        in (-1:1:oo) start,
        in Q^{n,n} distMatrix,
        in (-1:1:n) curPathIndex,
        in (-1:1:n) prevPathIndex,
        out (-1:1:n) nextPathIndex;
}
