package de.rwth.armin.modeling.autopilot.common;

component EnsureBoundsForBrakes {
  port in Q input_data, out Q output_data;

  instance EnsureBounds eb;

  connect 0.0 -> eb.lowerBound;
  connect 1.0 -> eb.upperBound;
  connect input_data -> eb.input_data;
  connect eb.output_data -> output_data;
}
