package de.rwth.armin.modeling.autopilot.motion;

component DecideEngineOrBrakes {
  port
    in Q error,
    in Q controlSignal,
    out Q engine,
    out Q brakes;

  implementation Math {
    engine = 0.0;
    brakes = 0.0;
    if (controlSignal > 0)
      engine = controlSignal / (3.6*7.460690450979396);
    else
      brakes = controlSignal;
    end
  }
}
