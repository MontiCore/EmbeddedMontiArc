package optimizer;

component ComplexVectorMultiplication2{
    ports in Q(-oo:oo)^{400,200} mat1,
          in Q(-oo:oo)^{200,100} mat2,
          in Q(-oo:oo)^{100,10} mat3,
          out Q(-oo:oo)^{10,200} matOut;


    implementation Math{
        Q^{1, 200} a = mat1(1,:);
        Q^{200, 1} b = mat2(:,1);
        Q^{1, 10} c = mat3(1,:);
        matOut = a*b*c;
    }

}
