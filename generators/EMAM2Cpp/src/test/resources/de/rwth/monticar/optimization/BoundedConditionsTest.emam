package de.rwth.monticar.optimization;

component BoundedConditionsTest{

    ports out Q xOut,
          out Q yOut;

    implementation Math{
        minimize
            Q x;
        in
            Q y = (x*x) - 2 * x + 1;
        subject to
            0 <= x <= 1;
            0 <= 2*x <= 1;
        end
        xOut = x;
        yOut = y;
    }
}
