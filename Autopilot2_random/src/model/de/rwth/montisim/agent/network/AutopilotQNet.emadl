package de.rwth.montisim.agent.network;

component AutopilotQNet {
    ports
        in Q^{29} state,
        out Q(-1:1)^{3} action;

    implementation CNN {

        state ->
        FullyConnected(units=500) ->
        Relu() ->
        FullyConnected(units=1000) ->
        Relu() ->
        FullyConnected(units=3) ->
        Tanh() ->
        action;
    }
}
