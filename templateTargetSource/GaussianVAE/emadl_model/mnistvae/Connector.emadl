package mnistvae;

component Connector{
    ports in Z(0:255)^{1,28,28} data,
          out Q(-oo:oo)^{2} z_mu,
          out Q(-oo:oo)^{2} z_logvar,
          out Q(-oo:oo)^{784} res;

    instance Encoder enc;
    instance Decoder dec;

    connect data -> enc.data;
    connect enc.z_mu -> z_mu;
    connect enc.z_logvar -> z_logvar;
    connect enc.z -> dec.sample;
    connect dec.res -> res;

}
