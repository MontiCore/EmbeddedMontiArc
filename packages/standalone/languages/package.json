  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "yarn run compile:clean && yarn run docs:clean",
    "build": "yarn run compile",
    "compile": "tsc --project compile.tsconfig.json",
    "compile:clean": "rimraf lib",
    "watch": "tsc --watch --project compile.tsconfig.json",
    "docs": "typedoc --tsconfig compile.tsconfig.json --options ../../../configs/typedoc.json",
    "docs:clean": "rimraf docs"
  },
  "dependencies": {
    "@theia/core": "latest",
    "@theia/languages": "latest",
    "@elysium/core": "^0.1.0",
    "@theia/monaco": "latest"
  },
  "devDependencies": {},
  "theiaExtensions": [
    {
      "frontend": "lib/browser/languages-frontend-module"
    }
  ]
}
