configuration FCN{
     num_epoch:2
     batch_size:8
     normalize:false
     context:gpu
     load_checkpoint:true
     loss:softmax_cross_entropy{
        loss_axis:1
     }
     optimizer:sgd{
         learning_rate:0.01
         learning_rate_decay:0.99
         step_size:500
         weight_decay:0.0001
     }
}
