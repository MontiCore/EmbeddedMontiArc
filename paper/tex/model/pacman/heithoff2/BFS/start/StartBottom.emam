package de.rwth.pacman.heithoff2.BFS.start;

component StartBottom{
    ports
        in (0m: 20m) pacManX,
        in (0m: 23m) pacManY,
        out (0m: 20m) startX,
        out (0m: 23m) startY,
        out Z startD;
        
    implementation Math {
        startX = pacManX;
        startY = round(pacManY + 0.51);
        startD = 1;
    }
}