package de.rwth.montisim.agent.network;

component AutopilotQNet {
    ports
        in Q^{50} state,
        out Q(-1:1)^{6} action;

    implementation CNN {

        state ->
        FullyConnected(units=500) ->
        Relu() ->
        FullyConnected(units=800) ->
        Relu() ->
        FullyConnected(units=6) ->
        Tanh() ->
        action;
    }
}
